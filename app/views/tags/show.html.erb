<div class="row">
  <h2>Item tagged "<%= @tag.name %>"</h2>
  <% @col_array.each do |col| %>

    <div class="col-md-4 scroll-col">
      <span class="test"></span>
      <% col.each do |i| %>
        <div class="board">
          <%= render i %>
        </div>
      <% end %>
    </div>

  <% end %>
</div>
