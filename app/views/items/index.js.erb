<% if defined?(@clear_board) %>
  $(".scroll-col").html('');
<% end %>

<% @col_array.each_with_index do |col, index| %>
  <% col.each do |tag| %>
    $('.scroll-col').eq(<%= index %>).append("<%= j render :partial => 'tags/board', :locals => { :tag => tag } %>");
  <% end %>
<% end %>

$('#loader').removeClass('loading');
$("#tag-search-loading").hide();
