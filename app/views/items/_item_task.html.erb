<!-- depricated?? -->
<div class="row item-task<%- if pin.completed_at %> completed<% end %>" data-id="<%= pin.id %>">
  <div class="col-md-2 pin-micro">
    <img src="<%= pin.item.thumb_url %>">
  </div>
  <div class="col-md-10">
    <%= link_to pin.item.name, stream_stream_pin_path(@stream, pin),{ :remote => true, :class => "pin-link", 'data-stream' => @stream.id, 'data-pin' => pin.id } %>
  </div>
</div>
